sqlcmd -S ..... -U .....-P ...... -d shallowcopy-sdh -s"|" -k1 -W -Q "SELECT p.ProductId, datepart(year, o.ActualCloseDate ) as [Year], datepart(month, o.ActualCloseDate ) as [Month], count(*) as [Count], sum(ActualValue) as [ActualValue]  FROM [dbo].[Hub_vw_Opportunity] o (nolock) JOIN [dbo].[Hub_vw_OpportunityProduct] op (nolock) on op.OpportunityId = o.OpportunityId JOIN [dbo].[Hub_vw_Product] p (nolock) on op.ProductId = p.ProductId JOIN [dbo].[Hub_vw_Account] a (nolock) on o.AccountId = a.AccountId   WHERE datepart(year, o.ActualCloseDate ) >= 2012 and a.Country = 'United States' and a.CurrencyId = 'FF2971D7-B412-E411-8D49-6C3BE5A82B68' and ActualValue is not null and ActualValue > 0 and ActualCloseDate is not null GROUP BY p.ProductId, datepart(year, o.ActualCloseDate ), datepart(month, o.ActualCloseDate )   ORDER BY [Year], [Month]" >oppty_by_sku.csv


